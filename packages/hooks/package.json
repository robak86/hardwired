{
  "name": "hardwired-hooks",
  "version": "0.8.4",
  "description": "Minimalistic, type-safe dependency injection solution for TypeScript",
  "author": "Tomasz Robaczewski <robak86@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/robak86/hardwired",
  "keywords": [
    "ioc",
    "dependency injection",
    "di",
    "container",
    "type-safe"
  ],
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/esm/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.js"
    },
    "./polyfill": "./dist/esm/polyfill/async_hooks.js"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "peerDependencies": {
    "hardwired": "0.8.4"
  },
  "files": [
    "dist",
    "README.md",
    "!/**/__tests__",
    "!/**/__test__",
    "!/**/*.test.ts",
    "!/**/*.test.tsx"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:robak86/hardwired.git"
  }
}