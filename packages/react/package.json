{
  "name": "hardwired-react",
  "version": "1.6.1",
  "author": "Tomasz Robaczewski <robak86@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/robak86/hardwired",
  "description": "Dependency injection container for React.",
  "keywords": [
    "ioc",
    "react",
    "dependency injection",
    "di"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "dependencies": {},
  "peerDependencies": {
    "hardwired": "1.6.1",
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "hardwired": "1.6.1"
  },
  "files": [
    "dist",
    "README.md",
    "!/**/__tests__",
    "!/**/__test__",
    "!/**/*.test.ts",
    "!/**/*.test.tsx"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:robak86/hardwired.git"
  }
}