{
  "compilerOptions": {
    "lib": ["ES2019.Array", "DOM"],
    "declarationMap": true,
    "sourceMap": true,
    "composite": true,
    "target": "es5",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": true,
    "declaration": true,
    "strict": true,
    "noImplicitAny": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "removeComments": true,
    "skipLibCheck": false
  }
}
