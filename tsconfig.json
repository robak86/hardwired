{
  "compilerOptions": {
    "composite": true,
    "declarationMap": true,
    "sourceMap": true,
    "target": "es5",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitReturns": true,
    "strictNullChecks": true,
    "declaration": true,
    "alwaysStrict": true,
    "strict": true,
    "strictFunctionTypes": false,
    "noImplicitAny": false,
    "keyofStringsOnly": false
  }
}
